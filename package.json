{
  "name": "emoji-stealer",
  "version": "0.2.4",
  "description": "Copy another discord server's emojis.",
  "main": "build/index.js",
  "scripts": {
    "start": "node .",
    "cs": "npm run build && node .",
    "build": "npm run clean && npm run build:ts",
    "build:ts": "tsc -p ./",
    "test": "npm run lint",
    "startmon": "ts-node-dev --respawn ./src/index.ts",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore src/",
    "lint:fix": "eslint --ext .js,.ts --ignore-path .gitignore src/ --fix",
    "watch": "tsc -w -p ./",
    "clean": "rimraf ./build && mkdir build",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@tmuniversal/weeb-wrapper": "^0.3.1",
    "app-root-path": "^3.0.0",
    "axios": "^0.19.2",
    "discord-akairo": "^8.0.0",
    "discord.js": "12.2.0",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.0",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/debug": "^4.1.5",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-gifsicle": "^5.2.0",
    "@types/imagemin-jpegtran": "^5.0.0",
    "@types/moment-duration-format": "^2.2.2",
    "@types/node": "^14.0.26",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.56",
    "tsc": "^1.20150623.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TMUniversal/Emoji-Stealer.git"
  },
  "keywords": [
    "discord",
    "discord.js",
    "bot",
    "discord",
    "bot"
  ],
  "author": "TMUniversal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TMUniversal/Emoji-Stealer/issues"
  },
  "homepage": "https://github.com/TMUniversal/Emoji-Stealer#readme"
}
